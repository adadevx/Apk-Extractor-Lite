<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.adadev.apkextractor">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:name="net.adadev.apkextractor.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="net.adadev.apkextractor.activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="net.adadev.apkextractor.activities.AppDetailActivity"
            android:label="Detail"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="net.adadev.apkextractor.activities.SettingActivity"
            android:label="@string/activity_settings_title"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="net.adadev.apkextractor.activities.FolderSelectorActivity"
            android:label="@string/activity_folder_selector_title"
            android:theme="@style/AppTheme.NoActionBar"/>
        <meta-data
            android:name="android.max_aspect"
            android:value="2.4" />

        <!-- Sample AdMob App ID: ca-app-pub-3940256099942544~3347511713 -->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>
    </application>

</manifest>